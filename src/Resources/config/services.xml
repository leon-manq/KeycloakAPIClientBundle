<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="ZoomAPIClientBundle\ZoomAPIClient">
            <argument key="$accountId">%env(resolve:ZOOM_ACCOUNT_ID)%</argument>
            <argument key="$clientId">%env(resolve:ZOOM_CLIENT_ID)%</argument>
            <argument key="$clientSecret">%env(resolve:ZOOM_CLIENT_SECRET)%</argument>
        </service>
    </services>
</container>
